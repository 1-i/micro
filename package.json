{
  "name": "micro-core",
  "version": "0.4.0",
  "description": "Micro core utilities",
  "main": "dist/index.js",
  "files": [
    "dist",
    "bin"
  ],
  "scripts": {
    "start": "gulp",
    "prepublish": "gulp transpile"
  },
  "bin": {
    "micro-serve": "./bin/micro-serve"
  },
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-async-to-generator",
      "syntax-async-functions"
    ]
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/zeit/micro-serve.git"
  },
  "keywords": [
    "micro",
    "server"
  ],
  "author": "zeit",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/zeit/micro-serve/issues"
  },
  "homepage": "https://github.com/zeit/micro-serve#readme",
  "devDependencies": {
    "babel-plugin-syntax-async-functions": "6.8.0",
    "babel-plugin-transform-async-to-generator": "6.8.0",
    "babel-preset-es2015": "6.13.2",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-cached": "1.1.0",
    "gulp-concat-util": "0.5.5"
  },
  "dependencies": {
    "babel-regenerator-runtime": "6.5.0",
    "isstream": "0.1.2",
    "media-typer": "0.3.0",
    "minimist": "1.2.0",
    "raw-body": "2.1.7"
  }
}
