{
  "name": "micro-cli",
  "version": "5.0.0",
  "description": "The command line utility for managing [micro]services",
  "repository": "zeit/micro-cli",
  "bugs": {
    "url": "https://github.com/zeit/micro-cli/issues"
  },
  "main": "dist/micro",
  "contributors": [
    {
      "name": "Guillermo Rauch",
      "email": "rauchg@zeit.co",
      "url": "http://rauchg.com"
    },
    {
      "name": "Leo Lamprecht",
      "email": "leo@zeit.co",
      "url": "https://leo.im"
    }
  ],
  "xo": {
    "esnext": true,
    "space": true,
    "semicolon": false,
    "rules": {
      "yoda": 0
    }
  },
  "greenkeeper": {
    "emails": false
  },
  "license": "MIT",
  "babel": {
    "presets": [
      "es2015"
    ],
    "plugins": [
      "transform-async-to-generator",
      "transform-runtime"
    ]
  },
  "homepage": "https://github.com/zeit/micro-cli",
  "dependencies": {
    "args": "1.3.1",
    "babel-plugin-module-alias": "1.6.0",
    "babel-plugin-transform-async-to-generator": "6.8.0",
    "babel-plugin-transform-runtime": "6.12.0",
    "babel-preset-es2015": "6.13.2",
    "babel-register": "6.11.6",
    "babel-runtime": "6.11.6",
    "media-typer": "0.3.0",
    "micro-core": "0.4.0",
    "raw-body": "2.1.7",
    "request": "^2.74.0"
  },
  "files": [
    "dist"
  ],
  "bin": {
    "micro": "dist/micro"
  },
  "scripts": {
    "start": "gulp",
    "prepublish": "gulp transpile",
    "pretest": "gulp transpile",
    "test": "xo"
  },
  "devDependencies": {
    "del": "2.2.1",
    "gulp": "3.9.1",
    "gulp-babel": "6.1.2",
    "gulp-cached": "1.1.0",
    "gulp-ext": "1.0.0",
    "gulp-task-listing": "1.0.1",
    "xo": "^0.16.0"
  }
}
